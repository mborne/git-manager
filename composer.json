{
    "name": "mborne/git-manager",
    "type": "project",
    "description": "Command line helpers to manage hosted git repositories",
    "autoload": {
        "psr-4": {
            "MBO\\GitManager\\": "src"
        },
        "classmap": [
            "app/AppKernel.php"
        ]
    },
    "bin": [
        "bin/console"
    ],
    "require": {
        "php": ">=7.2.5",
        "gitonomy/gitlib": "^1.0",
        "league/flysystem": "^1.0",
        "mborne/remote-git": "^0.2",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-kernel": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/process": "4.4.*",
        "symfony/routing": "4.4.*",
        "symfony/http-foundation": "4.4.*",
        "symfony/var-dumper": "4.4.*",
        "symfony/finder": "4.4.*",
        "symfony/filesystem": "4.4.*",
        "symfony/dependency-injection": "4.4.*",
        "symfony/config": "4.4.*",
        "symfony/var-exporter": "4.4.*",
        "symfony/cache": "4.4.*",
        "symfony/mime": "4.4.*",
        "symfony/yaml": "4.4.*"
    },
    "scripts": {},
    "config": {
        "sort-packages": true
    }
}