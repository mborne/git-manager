{
    "name": "mborne/git-manager",
    "type": "project",
    "description": "Command line helpers to manage hosted git repositories",
    "autoload": {
        "psr-4": {
            "MBO\\GitManager\\": "src"
        },
        "classmap": [
            "app/AppKernel.php"
        ]
    },
    "bin": [
        "bin/console"
    ],
    "require": {
        "gitonomy/gitlib": "^1.0",
        "league/flysystem": "^1.0",
        "mborne/remote-git": "^0.2",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-kernel": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/yaml": "4.4.*"
    },
    "scripts": {},
    "config": {
        "sort-packages": true
    },
    "extra": {
        "conflict": {
            "symfony/symfony": "*"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        },
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var"
    }
}